{
  "expo": {
    "name": "chessbro",
    "slug": "chessbro",
    "version": "1.0.0",
    "icon": "./assets/branding/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "chessbro",
    "splash": {
      "image": "./assets/branding/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/branding/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.daamin101.chessbro"
    },
    "web": {
      "favicon": "./assets/branding/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["node_modules/@expo-google-fonts/inter/900Black/Inter_900Black.ttf"]
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT_UP"
        }
      ]
    ],
  }
}
