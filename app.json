{
  "expo": {
    "name": "chessbro",
    "slug": "chessbro",
    "version": "1.0.0",
    "icon": "./assets/branding/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "chessbro",
    "splash": {
      "image": "./assets/branding/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/branding/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.daamin101.chessbro"
    },
    "web": {
      "favicon": "./assets/branding/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/varela-round/400Regular/VarelaRound_400Regular.ttf",
            "node_modules/@expo-google-fonts/nova-round/400Regular/NovaRound_400Regular.ttf",
            "node_modules/@expo-google-fonts/winky-rough/700Bold/WinkyRough_700Bold.ttf"
          ]
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT_UP"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8422afe6-ea5a-4d62-902c-ddffbd46d706"
      }
    },
    "owner": "daamin101"
  }
}
